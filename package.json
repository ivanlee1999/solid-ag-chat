{
  "name": "@torpago/solid-ag-chat",
  "version": "0.1.0",
  "description": "SolidJS chat components for AG-UI protocol integration with PydanticAI",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "files": [
    "dist",
    "src"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "solidjs",
    "chat",
    "ag-ui",
    "pydantic-ai",
    "components"
  ],
  "author": "Torpago",
  "license": "MIT",
  "peerDependencies": {
    "solid-js": "^1.8.0"
  },
  "devDependencies": {
    "solid-js": "^1.8.0",
    "tsup": "^8.0.0",
    "typescript": "^5.3.0",
    "vite": "^5.0.0",
    "vite-plugin-solid": "^2.10.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/livefire2015/solid-ag-chat.git"
  }
}
